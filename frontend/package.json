{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",

        "test:data": "jest --testPathPattern=__tests__/data",
        "test:utils": "jest --testPathPattern=__tests__/utils|jest --testPathPattern=__tests__/components",
        "test:components": "jest --testPathPattern=__tests__/components",

        "test:pages": "jest --testPathPattern=__tests__/pages",
        "test:ui": "jest --testPathPattern=__tests__/App|jest --testPathPattern=__tests__/layouts",
        "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
        "test:debug": "jest --detectOpenHandles --forceExit",
        "test:verbose": "jest --verbose",
        "test:update-snapshots": "jest --updateSnapshot"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.11",
        "chart.js": "^4.5.0",
        "dotenv": "^17.2.3",
        "morgan": "^1.10.0",
        "react": "^19.1.0",
        "react-chartjs-2": "^5.3.0",
        "react-dom": "^19.1.0",
        "react-router": "^7.6.3",
        "tailwindcss": "^4.1.11",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.29.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/express": "^5.0.3",
        "@types/jest": "^29.5.14",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.0.7",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.5.2",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-transform-stub": "^2.0.0",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": "^7.0.0"
    }
}
